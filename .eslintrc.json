{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "rules": {
        "react/prefer-stateless-function": "off",
        "object-curly-newline": [
            "error",
            {
                "ObjectExpression": "always",
                "ObjectPattern": {
                    "multiline": true
                }
            }
        ],
        "react/jsx-one-expression-per-line": false,
        "max-len": 0,
        "import/prefer-default-export": 1,
        "react/prop-types": 1
    }
}